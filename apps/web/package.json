{
  "name": "@repo/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm build && wrangler pages deploy",
    "//ref": "https://zenn.dev/chimame/articles/e5bf3a18b7d4de#remix-vite%3Adev-の注意点",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "//remix original start // start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "preview": "pnpm build && wrangler pages dev --persist-to ../../cf-local-worker",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.11.2",
    "@remix-run/cloudflare-pages": "2.11.2",
    "@remix-run/react": "2.11.2",
    "@repo/database": "workspace:*",
    "@repo/ui": "workspace:*",
    "drizzle-orm": "0.33.0",
    "isbot": "5.1.17",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240903.0",
    "@remix-run/dev": "2.11.2",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "autoprefixer": "10.4.20",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "postcss": "8.4.45",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vite-tsconfig-paths": "5.0.1",
    "wrangler": "3.75.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
